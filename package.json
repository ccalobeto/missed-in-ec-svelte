{
	"name": "missed-in-ec-svelte",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"python:dependencies": "pip install --upgrade pip && pip install -r requirements.txt",
		"data:generate": "python ./src/lib/assets/scripts/generate-data.py",
		"data:summary": "python ./src/lib/assets/scripts/summarize-data.py"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/d3-array": "^3.2.1",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-scale-chromatic": "^3.1.0",
		"@types/d3-shape": "^3.1.6",
		"autoprefixer": "^10.4.20",
		"d3-array": "^3.2.4",
		"d3-scale": "^4.0.2",
		"d3-scale-chromatic": "^3.1.0",
		"d3-shape": "^3.2.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"layerchart": "^0.59.6",
		"postcss": "^8.4.49",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.16",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"svelte-ux": "^0.76.0"
	}
}
